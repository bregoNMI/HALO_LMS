{
    "env": {
        "browser": true,
        "commonjs": true
    },
    "extends": "eslint-config-edx-es5",
    "rules": {
        "no-new": 0,
        "no-param-reassign": ["error", { "props": true, "ignorePropertyModificationsFor": ["ecommerce"] }],
        "no-underscore-dangle": ["error", { "allow": ["_super"] }]
    },
    "globals": {
        "gettext": true,
        "loadFixtures": true,
        "spyOnEvent": true,
        "ApplePaySession": true,
        "Stripe": true
    }
}
